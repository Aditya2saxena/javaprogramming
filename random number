import java.util.Scanner;
public class sum {
    public static void main(String[] arg){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int sum=0;
        while(num!=-1){
            sum=sum+num;
            num = sc.nextInt();
            
        }
        System.out.println(sum);
    }
}
