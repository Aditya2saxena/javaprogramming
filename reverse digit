import java.util.Scanner;
public class reversedigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();//2346
        int orignal=N;
        int rem;
        int rev=0;
        while(N>0){
            rem=N%10;
            rev=(rev*10)+rem;
            N=N/10;
        }
        
        System.out.println(rev);
    }
}
