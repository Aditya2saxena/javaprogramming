import java.util.Scanner;
public class counting {
    public static void main(String[] arg){
        Scanner sc =new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println("my n is " + n);
        int i=1;
        while (i<=n) {
            System.out.print(i+" ");
            i++;
        }
        //System.out.println(" ");
        
    }
}
