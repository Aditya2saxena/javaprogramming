import java.util.Scanner;
public class pattern{
    public static void main(String[] arg){
        Scanner sc = new Scanner(System.in);
        //int n = sc.nextInt();
        int r = sc.nextInt();
        int c = sc.nextInt();
        for(int i=1;i<=r;i++){
            for(int j=1;j<=c;j++){
                System.out.print("*");
            }
            System.out.println();
        }
          //System.out.println();
        
    }
}
